A,KAAA,EAAA;AACA,UAAA,OAAA,CAAA,KAAA,CAAA,KAAA;AACA,SALA;AAMA;AACA;AAVA;AAnBA,CAAA","sourcesContent":["<template>\r\n    <div class=\"row\">\r\n        <div class=\"col-md-12\">\r\n            <table class=\"table table-striped\">\r\n                <thead>\r\n                    <tr>\r\n                        <th>Nombre</th>\r\n                        <th>Correo</th>\r\n                        <th>Telefono</th>\r\n                        <th>Acciones</th>\r\n                    </tr>\r\n                </thead>\r\n                <tbody>\r\n                    <tr v-for=\"contacto in contactos\" :key=\"contacto.key\">\r\n                        <td>{{ contacto.nombre }}</td>\r\n                        <td>{{ contacto.correo }}</td>\r\n                        <td>{{ contacto.telefono }}</td>\r\n                        <td>\r\n                            <router-link :to=\"{name: 'edit', params: { id: contacto.key }}\" class=\"btn btn-primary\">Editar\r\n                            </router-link>\r\n                            <button @click.prevent=\"deleteUser(contacto.key)\" class=\"btn btn-danger\">Borrar</button>\r\n                        </td>\r\n                    </tr>\r\n                </tbody>\r\n            </table>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import { db } from '../firebaseDB';\r\n    \r\n    export default {\r\n        data() {\r\n            return {\r\n                contactos: []\r\n            }\r\n        },\r\n        created() {\r\n            db.collection('Contactos').onSnapshot((snapshotChange) => {\r\n                this.contactos = [];\r\n                snapshotChange.forEach((doc) => {\r\n                    this.contactos.push({\r\n                        key: doc.id,\r\n                        nombre: doc.data().nombre,\r\n                        correo: doc.data().correo,\r\n                        telefono: doc.data().telefono\r\n                    })\r\n                });\r\n            })\r\n        },\r\n        methods: {\r\n            deleteUser(id){\r\n              if (window.confirm(\"Realmente quieres borrar el contacto?\")) {\r\n                db.collection(\"Contactos\").doc(id).delete().then(() => {\r\n                    console.log(\"Contacto Borrado!\");\r\n                })\r\n                .catch((error) => {\r\n                    console.error(error);\r\n                })\r\n              }\r\n            }\r\n        }\r\n    }\r\n</script>\r\n\r\n<style>\r\n    .btn-primary {\r\n        margin-right: 12px;\r\n    }\r\n</style>"],"sourceRoot":"src/components"}]}